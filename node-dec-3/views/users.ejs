<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title><%= title %></title>
</head>
<body>
    <h1><%= title %></h1>
    <p><%= description %></p>
    <div class="user-list">
      <% users.forEach(function(user) { %>
        <div>
          <h3><%= user.username %></h3>
          <p>age: <%= user.age %></p>
        </div>
      <% }); %>
    </div>
</body>
</html>